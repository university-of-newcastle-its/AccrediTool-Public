{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Security": {
    "UserMappings": {
      "PLACEHOLDER_SECRET_NAME": {
        "Secret": "PLACEHOLDER_SECRET",
        "Role": "User,Admin"
      }
    },
    "JWT": {
      "ValidAudience": "http://localhost:4200",
      "ValidIssuer": "http://localhost:61955",
      "Secret": "PLACEHOLDER_JWT_SECRET",
      "ExpireHours": 3
    }
  }
}
